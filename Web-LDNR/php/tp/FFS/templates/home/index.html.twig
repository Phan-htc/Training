{% extends 'base.html.twig' %}
{% block title %}
tableau des pays
{% endblock %}

{% block main %}

{{ errors }}
<h1>Bienvenue sur la page d'accueil</h1>

<a href="/login">Se connecter</a>

<h3>Tableau des pays</h3>

<table class="alt3">
{% for res in results %}
  {% if loop.first %}
  <tr><th>id</th><th>nom</th><th>code</th><th>Drapeau</th></tr>
  {% endif %}
  <tr><td>{{res.id}}</td><td>{{res.nom}}</td><td>{{res.code}}</td><td>{{res.flag}}</td></tr>
{% endfor %}
</table>

{% endblock %}